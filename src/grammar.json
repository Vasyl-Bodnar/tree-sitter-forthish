{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "forthish",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\s*"
          },
          {
            "type": "SYMBOL",
            "name": "_expression"
          },
          {
            "type": "PATTERN",
            "value": "\\s*"
          }
        ]
      }
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "quote"
        },
        {
          "type": "SYMBOL",
          "name": "module"
        },
        {
          "type": "SYMBOL",
          "name": "function"
        },
        {
          "type": "SYMBOL",
          "name": "modcall"
        },
        {
          "type": "SYMBOL",
          "name": "word"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "[-+]?\\d+"
    },
    "constant": {
      "type": "PATTERN",
      "value": "true|false"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\([^)]*"
    },
    "quote": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\s*"
              },
              {
                "type": "SYMBOL",
                "name": "_expression"
              },
              {
                "type": "PATTERN",
                "value": "\\s*"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "module": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_expression"
              },
              {
                "type": "PATTERN",
                "value": "\\s+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "function": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": "\\s*"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_expression"
              },
              {
                "type": "PATTERN",
                "value": "\\s+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "modcall": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "word"
              },
              {
                "type": "STRING",
                "value": ":"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "fn",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        }
      ]
    },
    "word": {
      "type": "PATTERN",
      "value": "[^\\[\\]():;\\s]+"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
